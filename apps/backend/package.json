{
  "name": "@nft-market/backend",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "author": "",
  "license": "UNLICENSED",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "prebuild": "pnpm clean",
    "start:dev": "ts-node-dev src/app/bootstrap.ts",
    "start:prod": "NODE_ENV=production node dist/app/bootstrap.js",
    "start:swagger": "IS_SWAGGER_PROCESS=true ts-node-dev src/swagger/bootstrap.ts",
    "start:swagger:prod": "node dist/swagger/swagger-ui-server.js",
    "clean": "rimraf dist",
    "format:swagger": "prettier --write swagger.json",
    "postformat:swagger": "pnpm generate:swagger:html",
    "generate:swagger:html": "redoc-cli bundle swagger.json && mv redoc-static.html swagger.html",
    "typeorm:migration:generate": "NODE_ENV=development npx typeorm-ts-node-esm migration:generate -d ./src/type-orm.option.ts",
    "migration:generate": "pnpm build && pnpm typeorm:migration:generate",
    "postmigration:generate": "prettier --write src",
    "migration:revert": "pnpm build && NODE_ENV=development npx typeorm-ts-node-esm migration:revert -d ./src/type-orm.option.ts",
    "migration:run": "pnpm build && NODE_ENV=development npx typeorm-ts-node-esm migration:run -d ./src/type-orm.option.ts",
    "migration:run:prod": "NODE_ENV=production ts-node --cwd-mode node_modules/.bin/typeorm -f src/type-orm.option.ts migration:run",
    "lint": "eslint --fix 'src/**/{*.ts,*.tsx}'",
    "format": "prettier --write src",
    "hygen:module:new": "hygen module new",
    "posthygen:module:new": "yarn format"
  },
  "dependencies": {
    "@algoan/nestjs-logging-interceptor": "2.1.17",
    "@fastify/cookie": "^8.3.0",
    "@nestjs-addons/in-memory-db": "3.0.3",
    "@nestjs/axios": "2.0.0",
    "@nestjs/common": "9.4.3",
    "@nestjs/config": "2.3.4",
    "@nestjs/core": "9.4.3",
    "@nestjs/platform-express": "9.4.3",
    "@nestjs/swagger": "6.3.0",
    "@nestjs/terminus": "9.2.2",
    "@nestjs/typeorm": "9.0.1",
    "@nft-market/contract": "workspace:*",
    "aws-sdk": "2.1478.0",
    "aws4": "1.12.0",
    "axios": "1.5.1",
    "base64url": "3.0.1",
    "bcrypt": "5.1.1",
    "bodybuilder": "2.5.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "cookie-parser": "^1.4.6",
    "country-list": "^2.3.0",
    "csv-parse": "5.5.2",
    "csv-string": "4.1.1",
    "csvtojson": "2.0.10",
    "date-fns": "2.30.0",
    "date-fns-tz": "2.0.0",
    "dotenv": "16.0.3",
    "envalid": "7.3.1",
    "ethers": "5.7.2",
    "express": "4.18.2",
    "jsonwebtoken": "^9.0.0",
    "nest-aws-sdk": "3.0.1",
    "nestjs-command": "3.1.4",
    "node-fetch": "3.3.2",
    "pg": "8.11.3",
    "query-string": "8.1.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "4.4.1",
    "rxjs": "7.8.1",
    "swagger-ui-express": "4.6.3",
    "typeorm": "0.3.17",
    "uuid": "9.0.1",
    "winston": "3.11.0",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@nestjs/testing": "9.4.3",
    "@types/aws4": "1.11.5",
    "@types/bcrypt": "5.0.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/country-list": "^2.1.1",
    "@types/express": "4.17.20",
    "@types/geojson": "^7946.0.10",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "18.15.11",
    "@types/rimraf": "4.0.5",
    "@types/uuid": "9.0.6",
    "@types/yargs": "17.0.29",
    "eslint-plugin-prettier": "4.2.1",
    "redoc-cli": "0.13.21"
  }
}
