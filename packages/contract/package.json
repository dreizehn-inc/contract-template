{
  "name": "@web3-boiler/contract",
  "version": "0.0.1",
  "private": true,
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "typings": "esm/index.d.ts",
  "scripts": {
    "build": "run-p -l build:*",
    "build:cjs": "tsc --project . --module commonjs --outDir ./cjs",
    "build:esm": "tsc --project . --module es2015 --outDir ./esm",
    "clean": "rimraf esm cjs artifacts src/generated",
    "start:node": "hardhat node",
    "console": "hardhat console",
    "compile": "hardhat compile --show-stack-traces",
    "hardhat:clean": "hardhat clean",
    "test": "hardhat test",
    "deploy:local": "hardhat run scripts/deploy.ts --network localhost",
    "deploy:goerli": "hardhat run scripts/sample-deploy.ts --network goerli",
    "deploy:mumbai": "hardhat run scripts/sample-deploy.ts --network mumbai",
    "lint": "eslint --fix 'src/**/{*.ts,*.tsx}'"
  },
  "devDependencies": {
    "@ethersproject/abi": "5.7.0",
    "@ethersproject/providers": "5.7.2",
    "@nomiclabs/hardhat-ethers": "2.2.3",
    "@nomiclabs/hardhat-etherscan": "3.1.7",
    "@nomiclabs/hardhat-waffle": "2.0.6",
    "@openzeppelin/contracts": "4.9.3",
    "@openzeppelin/hardhat-upgrades": "1.28.0",
    "@typechain/ethers-v5": "10.2.1",
    "@typechain/hardhat": "6.1.6",
    "@types/chai": "4.3.7",
    "@types/mocha": "10.0.1",
    "@types/node": "18.15.11",
    "chai": "4.3.10",
    "dotenv": "16.0.3",
    "ethereum-waffle": "4.0.10",
    "ethers": "5.6.0",
    "hardhat": "2.18.0",
    "hardhat-gas-reporter": "1.0.9",
    "prettier-plugin-solidity": "1.1.3",
    "solc": "0.8.20",
    "solhint": "3.4.1",
    "solidity-coverage": "0.8.2",
    "ts-node": "10.9.1",
    "typechain": "8.1.1"
  },
  "dependencies": {
    "@chakra-ui/react": "2.5.5",
    "ethereumjs-util": "7.1.5"
  }
}
